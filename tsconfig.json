{
	"compilerOptions": {
		"target": "es5",
		"skipLibCheck": true,
		"outDir": "./dist/",
		"noImplicitAny": true,
		"baseUrl": ".",
		"resolveJsonModule": true,
		"paths": {
			"@banners/*": [ "banners/*" ],
			"@src/*": [ "src/*" ],
			"@test/*": [ "test/*" ],
			"@check-styles": [ "check-styles/*" ],
			"@environment/*": [ "src/environment/prod/*", "src/environment/dev/*" ]
		},
		"lib": [
			"dom",
			"es5",
			"scripthost",
			"es2015.promise"
		]
	}
}
